{
  "workbench.startupEditor": "newUntitledFile",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "workbench.iconTheme": "material-icon-theme",
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "remote.SSH.remotePlatform": {
    "AWS_EC2_first1": "linux",
    "EC2Basic": "linux",
    "Hakobune": "linux",
    "Hakobune2": "linux",
    "aws-2": "linux",
    "Aws-Ubuntu": "linux",
    "MacAnjiDev": "macOS"
  },
  // "workbench.editorAssociations": {
  //   "*.ipynb": "jupyter.notebook.ipynb"
  // },
  "window.title": "üëåü§üüèª${dirty}${activeEditorShort}${separator}¬Ø\\_(„ÉÑ)_/¬Ø",
  "security.workspace.trust.enabled": false,
  "explorer.confirmDragAndDrop": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.defaultProfile.osx": "zsh",
  // "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "DejaVu Sans Mono for Powerline",
  "explorer.confirmDelete": false,
  "vim.overrideCopy": false,
  "vim.highlightedyank.enable": true,
  "vim.handleKeys": {
    "<C-w>": false,
    "<C-c>": false,
    "<C-k>": false
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "editor.inlineSuggest.enabled": true,
  "editor.suggestSelection": "first",
  "workbench.tree.indent": 16,
  "workbench.colorCustomizations": {
    "tree.indentGuidesStroke": "#32fb05",
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "extensions.ignoreRecommendations": true,
  "bracketPairColorizer.depreciation-notice": false,
  "css.lint.unknownAtRules": "ignore",
  "git.autofetch": true,

  // ANCHOR Customize anchor
  "commentAnchors.tags.provideAutoCompletion": true,
  "commentAnchors.tags.displayInSidebar": false,
  "commentAnchors.tags.displayInRuler": false,
  "commentAnchors.tags.displayLineNumber": false,
  "commentAnchors.tagHighlights.enabled": true,
  "commentAnchors.scrollPosition": "top",
  "commentAnchors.workspace.lazyLoad": true,
  "commentAnchors.tags.list": [
    {
      "tag": "_HANDLER",
      "scope": "file",
      "iconColor": "#689f38",
      "highlightColor": "#8bc34a",
      "styleComment": false
    },

    {
      "tag": "AXIOS",
      "scope": "file",
      "iconColor": "#ff8f00",
      "highlightColor": "#ff6f00",
      "styleComment": false
    },

    {
      "tag": "SOCKET",
      "scope": "file",
      "iconColor": "#039be5",
      "highlightColor": "#03a9f4",
      "styleComment": false
    },

    {
      "tag": "_GET",
      "scope": "file",
      "iconColor": "#f06292",
      "highlightColor": "#f06292",
      "styleComment": false
    },
    {
      "tag": "_POST",
      "scope": "file",
      "iconColor": "#b39ddb",
      "highlightColor": "#b39ddb",
      "styleComment": false
    },
    {
      "tag": "_PUT",
      "scope": "file",
      "iconColor": "#42a5f5",
      "highlightColor": "#42a5f5",
      "styleComment": false
    },
    {
      "tag": "_DEL",
      "scope": "file",
      "iconColor": "#ff5252",
      "highlightColor": "#ff5252",
      "styleComment": false
    },

    {
      "tag": "API",
      "scope": "file",
      "iconColor": "#af52bf",
      "highlightColor": "#af52bf",
      "styleComment": false
    },
    {
      "tag": "_CASE",
      "scope": "file",
      "iconColor": "#651fff",
      "highlightColor": "#651fff",
      "styleComment": false
    },
    {
      "tag": "LOGIC",
      "scope": "file",
      "iconColor": "#76ff03",
      "highlightColor": "#76ff03",
      "styleComment": false
    },
    {
      "tag": "_LOG",
      "scope": "file",
      "iconColor": "#76ff03",
      "highlightColor": "#76ff03",
      "styleComment": false
    }
  ],
  "editor.cursorSmoothCaretAnimation": true,
  "gitlens.defaultDateShortFormat": null,
  "gitlens.defaultTimeFormat": null,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "workbench.colorTheme": "Dracula",
  "blockman.n01LineHeight": 0,
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": true,
  "editor.inlayHints.enabled": true,
  // ANCHOR GitLen
  "gitlens.hovers.enabled": false,
  "svelte.enable-ts-plugin": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "window.zoomLevel": -1,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "typescript": false,
    "typescriptreact": false,
    "javascript": false
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".graphql",
        "isAsync": true,
        "cmd": "tsc"
      },
      {
        "match": "schema.prisma",
        "isAsync": true,
        "cmd": "npx prisma migrate dev --name any_name"
      }
    ]
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  }
}

